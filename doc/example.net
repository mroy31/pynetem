[config]
image_dir = images
config_dir = configs

[nodes]

[[R1]]
type = qemu.juniper
console = 2001
if_numbers = 2
if0 = sw.Switch1
if1 = User2.1

[[User1]]
type = qemu.squeeze
memory = 192
console = 3001
if_numbers = 1
if0 = Switch1

[[R2]]
type = docker.xorp
if_numbers = 1
if0 = Switch2

[[User2]]
type = docker.host
save_state = false
if_numbers = 2
if0 = Switch2
if1 = R1.1


[switches]

[[Switch1]]
type = vde
tap = true

[[Switch2]]
type = ovs